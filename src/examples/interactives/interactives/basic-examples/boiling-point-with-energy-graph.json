{
  "title": "Boiling Point with Energy Graph",
  "about": "Interactive example",
  "models": [
    {
      "id": "boiling-point-with-clock$0",
      "url": "/imports/legacy-mw-content/converted/conversion-and-physics-examples/boiling-point-with-clock$0.json",
      "viewOptions": {
        "controlButtons": "play_reset_step",
        "modelTimeLabel": true
      },
      "onLoad": "set({ temperature: 200 });"
    }
  ],
  "components": [
    {
      "type": "button",
      "id": "cool",
      "text": "Cool",
      "action": "adjustTemperature(0.9);"
    },
    {
      "type": "button",
      "id": "heat",
      "text": "Heat",
      "action": "adjustTemperature(1.1); limitHighTemperature(4000);"
    },
    {
      "type": "slider",
      "id": "temperature-slider",
      "min": 0,
      "max": 4000,
      "displayValue": "return format('1f')(value)",
      "labels": [
        {
          "value": 0,
          "label": "0 K"
        },
        {
          "value": 4000,
          "label": "4000 K"
        }
      ],
      "steps": 400,
      "title": "Temperature",
      "property": "temperature"
    },
    {
      "type": "energyGraph",
      "id": "ke-chart",
      "dimensions": {
        "width": 500,
        "height": 296
      },
      "options": {
        "ymin": -15,
        "ymax": 15,
        "xmax": 30
      }
    }
  ],
  "layout": {
    "rightwide": [
      "ke-chart"
    ],
    "bottom": [
      "heat",
      "cool",
      "temperature-slider"
    ]
  }
}