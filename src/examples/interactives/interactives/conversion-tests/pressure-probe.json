{
  "title": "Testing: Pressure Probe",
  "about": "Interactive example",
  "model": {
    "url": "/imports/legacy-mw-content/converted/conversion-and-physics-examples/pressure-probe$0.json",
    "viewOptions": {
      "controlButtons": "play_reset_step",
