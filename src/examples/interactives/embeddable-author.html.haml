    = ANALYTICS
  %body
    #editor
      #editor-content.extras-item-content
        .extras-item-controls
          %button#update-interactive-button Update Interactive
          %a#json-model-link{:href => "", :class => "opens-in-new-window", :target => "_blank"} Model JSON
        %textarea#interactive-text-area
    #content
      #content-banner
        #credits
          #credits-link.link The Concord Consortium
        #about
          #about-link.link About
        #share
          #share-link.link Share
      #viz.top
        #interactive-container

    #credits-pane
      #credits-pane-banner
        %span#credits-pane-title About
        #credits-pane-close X
      #credits-content

    #about-pane
      #about-pane-banner
        %span#about-pane-title About
        #about-pane-close X
      #about-content

    #share-pane
      #share-pane-banner
        %span#share-pane-title Share
        #share-pane-close X
      #share-content
        %p
          Paste this
          %a#share-embeddable-link{:href => "embeddable.html", :class => "opens-in-new-window", :target => "_blank"} link
          in email or IM.
        %textarea#share-embeddable-link-content
        %p< Paste HTML to embed in website or blog.
        #share-iframe-form<
          Select Size:
          %select#share-select-iframe-size
            %option{:value => "smaller"} 30% smaller
            %option{:value => "actual", :selected => true} actual
            %option{:value => "larger"} 50% larger
          %textarea#share-iframe-content
        #share-license
          %strong Copyright Â© 2013&nbsp;
          %a#share-license-link{:href => "http://concord.org", :class => "opens-in-new-window", :target => "_blank"}> The Concord Consortium
          \. All rights reserved. The software is licensed under&nbsp;
          %a#share-license-link{:href => "http://opensource.org/licenses/BSD-2-Clause", :class => "opens-in-new-window", :target => "_blank"}> Simplified BSD
          ,
          %a#share-license-link{:href => "http://opensource.org/licenses/MIT", :class => "opens-in-new-window", :target => "_blank"} MIT
          or
          %a#share-license-link{:href => "http://opensource.org/licenses/Apache-2.0", :class => "opens-in-new-window", :target => "_blank"} Apache 2.0
          licenses. Please provide attribution to the Concord Consortium and the URL&nbsp;
          %a#share-license-link{:href => "http://concord.org/", :class => "opens-in-new-window", :target => "_blank"}> http://concord.org
          \.
